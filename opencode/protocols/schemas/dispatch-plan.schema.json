{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "DispatchPlan",
  "type": "object",
  "required": [
    "batches",
    "advanced_reserve_tasks",
    "notes"
  ],
  "properties": {
    "protocol_version": {
      "type": "string",
      "pattern": "^[0-9]+\\.[0-9]+$"
    },
    "batches": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "batch_id": {
            "type": "string"
          },
          "task_ids": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "assigned_executor": {
            "type": "string"
          },
          "parallel": {
            "type": "boolean"
          },
          "notes": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "additionalProperties": true
      }
    },
    "advanced_reserve_tasks": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "notes": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": false
}

