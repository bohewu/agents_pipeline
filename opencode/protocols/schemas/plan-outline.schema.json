{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "PlanOutline",
  "type": "object",
  "required": [
    "milestones",
    "dependencies",
    "deliverables"
  ],
  "properties": {
    "protocol_version": {
      "type": "string",
      "pattern": "^[0-9]+\\.[0-9]+$"
    },
    "milestones": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "dependencies": {
      "type": "object",
      "additionalProperties": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "deliverables": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": false
}
