{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "RepoFindings",
  "type": "object",
  "required": [
    "entry_points",
    "key_files",
    "relevant_patterns",
    "constraints_found",
    "risk_notes",
    "open_questions"
  ],
  "properties": {
    "protocol_version": {
      "type": "string",
      "pattern": "^[0-9]+\\.[0-9]+$"
    },
    "entry_points": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "key_files": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "relevant_patterns": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "constraints_found": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "risk_notes": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "open_questions": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": false
}
